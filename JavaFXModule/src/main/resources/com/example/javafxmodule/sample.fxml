<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1268.0" spacing="20.0" styleClass="background" stylesheets="@application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxmodule.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <GridPane prefHeight="501.0" prefWidth="1228.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="1038.0" minWidth="10.0" prefWidth="265.0" />
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="937.0" minWidth="10.0" prefWidth="641.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="937.0" minWidth="10.0" prefWidth="267.0" />
     </columnConstraints>
     <rowConstraints>
         <RowConstraints maxHeight="228.0" minHeight="10.0" prefHeight="153.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="228.0" minHeight="0.0" prefHeight="153.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="251.0" minHeight="7.0" prefHeight="149.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="337.0" minHeight="10.0" prefHeight="159.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <TextField fx:id="idInput" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         <Label prefHeight="18.0" prefWidth="659.0" text="E-mail" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <Label alignment="CENTER" prefHeight="18.0" prefWidth="763.0" text="Inscription" GridPane.columnIndex="1">
            <font>
               <Font name="Lato Bold" size="19.0" />
            </font>
         </Label>
      </children>
   </GridPane>

    <Label fx:id="welcomeText" />
    <Button fx:id="buttonTest" onAction="#pressButton" prefHeight="71.0" prefWidth="235.0" text="Ca flingue fort">
      <font>
         <Font name="Lato Regular" size="12.0" />
      </font>
   </Button>
</VBox>
